gos_result_t APP_ButtonInit (void_t)
{
	/*
	 * Local variables.
	 */
	gos_result_t buttonInitResult = GOS_SUCCESS;

	/*
	 * Function code.
	 */
	buttonInitResult = gos_taskRegister(&buttonTaskDesc, NULL);

	return buttonInitResult;
}

